#include "stdafx.h"
#include "conio.h"
#include <iostream>

using namespace std;

void main()

{
    int fac,i,n;
    cout<<"Ingrese hasta que numero multiplicar"<<endl;
    cin>>n;
    fac=1;
    i=1;
    while(i<=n)
    {
        fac=fac*i;
        i++;
    }
    cout<<"El resultado sera"<<fac<<endl,
    getch();
}

#include "stdafx.h"
#include "conio.h"
#include <iostream>

using namespace std;

void main()

{
    int fac,i,n;
    cout<<"Ingrese hasta que numero multiplicar"<<endl;
    cin>>n;
    fac=1;
    for(i=1;i<=n;i++)
    
        fac=fac*i;
    
    cout<<"El resultado sera"<<fac<<endl,
    getch();
}
